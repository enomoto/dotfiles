[user]
  name    = enomoto
  email   = kenta.enomoto@iridge.jp
[alias]
  ad      = add
  b       = branch
  c      = checkout
  d       = diff
  dc      = diff --cached
  g       = grep --line-number --heading --break --show-function
  l       = log --graph -n 20 --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset'
  ls      = ls-files
  s       = status
[core]
  editor  = vim -c \"set fenc=utf-8\"
